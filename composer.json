{
    "name": "martiadrogue/mpwarfwk",
    "description": "A MVC framework.",
    "keywords": ["framework", "mvc"],
    "homepage": "https://github.com/martiadrogue/mpwarfwk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martí Adrogué",
            "email": "marti.adrogue@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-memcached": "~2.2",
        "symfony/yaml": "~2.6",
        "twig/twig": "~1.18",
        "smarty/smarty": "~3.1",
        "phpmailer/phpmailer": "~5.2"

    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd": "~2.2",
        "sebastian/phpcpd": "~2.0",
        "pdepend/pdepend": "~2.1",
        "phploc/phploc": "~2.1",
        "phpunit/phpunit": "~4.6",
        "theseer/phpdox": "~0.8"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
          "Com\\Martiadrogue\\Mpwarfwk\\": "src/Com/Martiadrogue/Mpwarfwk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Com\\Martiadrogue\\MpwarfwkTest\\": "tests/Com/Martiadrogue/MpwarfwkTest/Unit"
        }
    }
}
